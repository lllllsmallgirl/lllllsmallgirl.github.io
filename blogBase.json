4\u65ad\u662f\u5426\u5b58\u5728\u73af\r\n```js\r\nfunction hasCycle(graph) {\r\n  const visited = new Set();\r\n  const stack = new Set();\r\n\r\n  function dfs(node) {\r\n    if (stack.has(node)) {\r\n      return true; // \u5f53\u524d\u8282\u70b9\u5728\u9012\u5f52\u5806\u6808\u4e2d\uff0c\u8868\u793a\u5b58\u5728\u73af\r\n    }\r\n    if (visited.has(node)) {\r\n      return false; // \u5f53\u524d\u8282\u70b9\u5df2\u7ecf\u88ab\u8bbf\u95ee\u8fc7\uff0c\u4e0d\u5b58\u5728\u73af\r\n    }\r\n\r\n    visited.add(node); // \u6807\u8bb0\u5f53\u524d\u8282\u70b9\u4e3a\u5df2\u8bbf\u95ee\r\n    stack.add(node); // \u5c06\u5f53\u524d\u8282\u70b9\u6dfb\u52a0\u5230\u9012\u5f52\u5806\u6808\u4e2d\r\n\r\n    for (let neighbor of graph[node] || []) {\r\n      if (dfs(neighbor)) {\r\n        return true;\r\n      }\r\n    }\r\n\r\n    stack.delete(node); // \u5f53\u524d\u8282\u70b9\u9012\u5f52\u7ed3\u675f\uff0c\u4ece\u5806\u6808\u4e2d\u79fb\u9664\r\n    return false;\r\n  }\r\n\r\n  for (let node in graph) {\r\n    if (dfs(node)) {\r\n      return true;\r\n    }\r\n  }\r\n\r\n  return false;\r\n}\r\n\r\n```\r\n\r\n\r\n\u8f85\u52a9\u51fd\u6570 dfs\u904d\u5386\u6811\r\n```js\r\nconst findAllNode = (tree) => {\r\n    const nodes = [];\r\n    const dfs = (node) => {\r\n        nodes.push(node.id);\r\n        nodes.children.forEach(child => dfs(child));\r\n    }\r\n    dfs(tree);\r\n    return nodes;\r\n}\r\n```\u3002", "top": 0, "createdAt": 1719490283, "style": "", "script": "", "head": "", "ogImage": "https://avatars.githubusercontent.com/u/70518807?v=4", "createdDate": "2024-06-27", "dateLabelColor": "#bc4c00"}, "P6": {"htmlDir": "docs/post/zhan-kai---shou-qi- -xiao-guo.html", "labels": ["documentation"], "postTitle": "\u5c55\u5f00/\u6536\u8d77 \u6548\u679c", "postUrl": "post/zhan-kai---shou-qi-%20-xiao-guo.html", "postSourceUrl": "https://github.com/lllllsmallgirl/lllllsmallgirl.github.io/issues/6", "commentNum": 0, "wordCount": 678, "description": "## \u4f7f\u7528css\u5b9e\u73b0\u5c55\